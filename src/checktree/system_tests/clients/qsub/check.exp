   lappend ret "test!test"
   lappend ret "test,test"
   lappend ret "test;test"
   lappend ret "test.test"
   lappend ret "testtest"
   lappend ret "test=test"
   lappend ret "test%test"
   lappend ret "test|test"
   lappend ret "\"test\""

   if {$option == "-A"} {
      lappend ret "1test"
   }

   return $ret
}

